{
  "name": "@commaai/pandajs",
  "version": "0.1.4",
  "description": "JavaScript interface for communicating with Panda devices.",
  "main": "lib/index.js",
  "scripts": {
    "test": "tape -r babel-register './src/**/*test.js' | tap-spec",
    "watch": "nodemon -w src --exec npm run build",
    "build": "babel ./src/ --ignore '**.test.js','test.js' --out-dir ./lib/",
    "prepare": "npm run build",
    "tape": "tape",
    "tap-spec": "tap-spec",
    "babel": "babel"
  },
  "keywords": [
    "CAN",
    "panda",
    "comma.ai",
    "javascript"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "escape-string-regexp": "^1.0.5",
    "tap-spec": "^4.1.1",
    "tape": "^4.9.0"
  },
  "repository": "https://github.com/commaai/pandajs",
  "author": "Chris Vickery <chrisinajar@gmail.com>",
  "dependencies": {
    "ap": "^0.2.0",
    "can-message": "^0.1.0",
    "es6-sleep": "^2.0.2",
    "is-browser": "^2.0.1",
    "is-node": "^1.0.2",
    "is-promise": "^2.1.0",
    "performance-now": "^2.1.0",
    "raf": "^3.4.0",
    "thyming": "^0.1.1",
    "usb": "^1.3.1",
    "weakmap-event": "^2.0.7"
  }
}
